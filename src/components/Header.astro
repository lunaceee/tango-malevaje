---
import { loadQuery } from "../../sanity/lib/load-query";
import { headerQuery } from "../../sanity/lib/queries";
import HeaderComponent from "./HeaderComponent";
interface Header {
  _id: string;
  title: string;
  logo?: {
    url: string;
  };
  navigation: {
    _ref: string;
    _type: "reference";
  };
}

const { data: header } = await loadQuery<Header>({
  query: headerQuery,
});
---

<HeaderComponent header={header} />
