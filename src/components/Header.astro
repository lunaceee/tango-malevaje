---
import { loadQuery } from "../../sanity/lib/load-query";
import { headerQuery } from "../../sanity/lib/queries";
import { Header } from "./Header";
interface Header {
  _id: string;
  _type: "header";
  logo?: {
    _type: "image";
    asset: {
      _ref: string;
      _type: "reference";
    };
  };
  navigation: {
    _ref: string;
    _type: "reference";
  };
}

const { data: header } = await loadQuery<Header>({
  query: headerQuery,
});
---

<Header header={header} />
